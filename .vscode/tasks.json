{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "ðŸš€ Startup API",
            "type": "shell",
            "command": "./startup.sh",
            "isBackground": true,
            "presentation": {
                "reveal": "always",
                "panel": "dedicated"
            },
            "problemMatcher": {
                "pattern": {
                    "regexp": "^(.*)$",
                    "file": 1,
                    "location": 2,
                    "message": 3
                },
                "background": {
                    "activeOnStart": true,
                    "beginsPattern": "^.*Iniciando Spring Boot.*",
                    "endsPattern": "^.*Started.*Application.*"
                }
            },
            "runOptions": {
                "runOn": "folderOpen"
            }
        }
    ]
}